<head>
    <title>Update Quote</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <script src="https://code.jquery.com/jquery-3.3.1.min.js"></script>
    <script src="https://kit.fontawesome.com/a2c9d7080d.js" crossorigin="anonymous"></script>
  <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js" integrity="sha384-ChfqqxuZUCnJSK3+MXmPNIyE6ZbWh2IMqE241rYiqJxyMiZ6OW/JmZQ5stwEULTy" crossorigin="anonymous"></script>
  <link href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">
  </head>
 <body >
    <!-- include header -->
    <%- include('include/header') %>
    <!-- /include header -->

    <div class="main-content">
        <div class="column-1" >
          <div class="vertical-menu">
            <a href="/admin-dashboard"  >Admin Dashboard</a>
            <a href="/accepted-quotes-admin" class="active">Accepted Quotes</a>
            <a href="/freight-charges">Freight Charges</a>
            <a href="/custom-quote-requests">Custom Quote Requests</a>
          </div>
        </div>
        <div class="column-2" >
    
            <div class="card">
                <div class="card-body">
                   
                <div>
                    <h2 >Update Quote</h2>
                </div>
        
                <form method="POST" id="update_quote">
                    <hr>
                    <h4>Customer Information</h4>
                    <div class="form-group2">
                      
                      <div class="form-cell">
                        <label for="name">Name</label>
                        <input  type="text" class="form-control" readonly id="name" aria-describedby="emailHelp" placeholder="Enter Name:" name="name" value="<%= quote.name%>">
                      </div>
  
                      
                    </div>
  
                    <hr>
                    <h4>Origin</h4>
                    <div class="form-group2">
                      
                      <div class="form-cell">
                        <label for="pickupcountry" >Origin Country:</label>
                        <input type="text" class="form-control" readonly id="pickupcountry" name="pickupcountry" value="<%= quote.pickupcountry%>">
                      </div>
                      
                      <div class="form-cell">
                        <label for="loadingPort">Loading Port / Airport:</label>
                        <input type="text" class="form-control" readonly id="loadingPort" name="loadingPort" value="<%= quote.loadingPort%>">
                      </div>
  
                      <div class="form-cell">
                        <label for="incoterms">Incoterms:</label>
                        <input type="text" class="form-control" readonly id="incoterms" name="incoterms" value="<%= quote.incoterms%>">
                      </div>
                      
                    </div>
  
                    <div class="form-group2 ">
                      <div class="form-cell">
                        <label for="pickupAddress">Origin Address:</label>
                        <input type="String" class="form-control"  readonly id="pickupAddress" name="pickupAddress" value="<%= quote.pickupAddress%>">
                      </div>
                    </div>
                    
                    <hr>
                    <h4>Destination</h4>
                    <div class="form-group2">
                      
                      <div class="form-cell">
                        <label for="deliveryAddress">Delivery Address:</label>
                        <input  type="text" readonly class="form-control"  id="deliveryAddress" aria-describedby="emailHelp" placeholder="Enter Name:" name="deliveryAddress" value="<%= quote.deliveryAddress%>">
                      
                      </div>
                      
                      <div class="form-cell">
                        <label for="shippingMethod">Shipping Method:</label>
                        <input type="text" class="form-control" readonly id="shippingMethod" name="shippingMethod" value="<%= quote.shippingMethod%>" >
                      </div>
                      
                      
                      <div class="form-cell">
                        <label for="shippingSpeed">Shipping Speed:</label>
                        <input type="text" class="form-control" readonly id="shippingSpeed" name="shippingSpeed" value="<%= quote.shippingSpeed%>">
                      </div>
                      
                    </div>
  
                    <div class="form-group2">
                      <div class="form-cell airFreight">
                        <label for="noOfBoxes">No. of  Boxes:</label>
                        <input type="number" class="form-control" readonly id="noOfBoxes" name="noOfBoxes" value="<%= quote.noOfBoxes%>">
                      </div>
  
                      <div class="form-cell airFreight">
                        <label for="withPallet">Should we put your boxes in a pallet? Additional 20kg per pallet</label>
                        <input type="text" class="form-control" readonly id="withPallet" name="withPallet" value="<%= quote.withPallet%>">
                      </div>
  
                      <div class="form-cell seaFreight">
                        <label for="containerSize">Container Type:</label>
                        <input type="text" class="form-control" readonly id="containerSize" name="containerSize" value="<%= quote.containerSize%>">
                      </div>
  
                      <script>
  
                      </script>
    
                    </div>
  
                    
                    <hr>
                    <h4>Dimensions / Box</h4>
                    <div class="form-group2">
                      
    
                      <div class="form-cell">
                        <label for="length">Length (cm):</label>
                        <input type="number" class="form-control" readonly id="length" name="length" value="<%= quote.length%>">
                      </div>
    
                      <div class="form-cell">
                        <label for="width">Width  (cm):</label>
                        <input type="number" class="form-control" readonly id="width" name="width" value="<%= quote.width%>">
                      </div>
    
                      <div class="form-cell">
                        <label for="height">Height  (cm):</label>
                        <input type="number" class="form-control" readonly id="height" name="height" value="<%= quote.height%>">
                      </div>
    
                      <div class="form-cell">
                        <label for="weight">Weight (kg):</label>
                        <input type="number" class="form-control" readonly id="weight" name="weight" value="<%= quote.weight%>" >
                      </div>
    
                    </div>
                    
                    
                    <hr>
                    <h4>Chargeable Weight</h4>
                    <div class="form-group2">
                      <div class="form-cell">
                        <br><span><b> <input type="text" style="display: inline;border:none;" id="chargeableWeight" name="chargeableWeight" value="<%= quote.chargeableWeight%>"></b> kg</span>
                      </div>
                      
                    </div>
  
  
                    <hr>
                    <h4>Charges</h4>
                    <div class="form-group2">
                      
    
                      <div class="form-cell">
                        <label for="totalOriginCharges">Origin Charges AUD</label>
                        <input type="number" class="form-control" readonly id="totalOriginCharges" name="totalOriginCharges" value="<%= quote.totalOriginCharges%>">
                      </div>
    
                      <div class="form-cell">
                        <label for="localPortCharges">Local Port Charges AUD</label>
                        <input type="number" class="form-control" readonly id="localPortCharges" name="localPortCharges" value="<%= quote.localPortCharges%>" >
                      </div>
    
                      <div class="form-cell">
                        <label for="localTransportCharges">localTransportCharges AUD</label>
                        <input type="number" class="form-control" readonly id="localTransportCharges" name="localTransportCharges" value="<%= quote.localTransportCharges%>" >
                      </div>
    
                    </div>
  
                    <hr>
                    <h4>Estimated Quote Price</h4>
                    <div class="form-group2">
                      <div class="form-cell">
                        <br><span><b> USD <input type="text" style="display: inline;border:none;" id="quoteprice" name="quoteprice" value="<%= quote.quoteprice%>"></b></span>
                      </div>

              
                    </div>

                    <h4>Update Quote Status</h4>
                    <div class="form-group2">
                      <div class="form-cell">
                        <label for="status">Status</label>
                        <input type="text" class="form-control"  id="status" name="status" value="<%= quote.status%>" >
                      </div>
                      
                    </div>
                    <button type="submit" class="btn btn-save" formaction="/update-status">Save Quote</button>
                    
                    
                  </form>
            </div>
        </div>
        </div>
    </div>

    
    
</body>
