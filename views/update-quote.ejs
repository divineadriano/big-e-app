<head>
    <title>Update Quote</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <script src="https://code.jquery.com/jquery-3.3.1.min.js"></script>
    <script src="https://kit.fontawesome.com/a2c9d7080d.js" crossorigin="anonymous"></script>
  <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js" integrity="sha384-ChfqqxuZUCnJSK3+MXmPNIyE6ZbWh2IMqE241rYiqJxyMiZ6OW/JmZQ5stwEULTy" crossorigin="anonymous"></script>
  <link href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">
  </head>
 <body >
    <!-- include header -->
    <%- include('include/header') %>
    <!-- /include header -->

    <div class="main-content">
        <div class="column-1" >
            <div class="vertical-menu">
                <a href="/quote-portal" >Quote Calculator</a>
                <a href="/accepted-quotes">Accepted Quotes</a>
                <a href="/drafted-quotes" class="active">Draft Quotes</a>
                <a href="/my-quote-requests">Requested Quotes</a>
            </div>
        </div>
        <div class="column-2" >
    
            <div class="card">
                <div class="card-body">
                   
                <div>
                    <h2 >Update Quote</h2>
                </div>
        
                        <!-- add user form -->
                        <!-- form handling -->
                <form method="POST" id="update_quote">
                        <div class="form-group">
                            <label for="createdAt" >Created At</label>
                            <input type="text" class="form-control" name="createdAt" value="<%= quote.createdAt %>" >
                        </div>
        
                        <div class="form-group">
                            <label for="name" >Name</label>
                            <input type="text" class="form-control" name="name" value="<%= quote.name %>" >
                        </div>
    
                        <div class="form-group2">
                            <div class="form-cell">
                                <label for="pickupcountry" >Pickup Country:</label>
                                <input type="text" class="form-control" name="pickupcountry" value="<%= quote.pickupcountry %>" >
                            </div>
                            
                            <div class="form-cell">
                                <label for="pickupsuburb" >Pickup Suburb:</label>
                                <input type="text" class="form-control" name="pickupsuburb" value="<%= quote.pickupsuburb %>" >
                            </div>
                        </div>
    
                        <div class="form-group">
                            <label for="destination">Destination Country:</label>
                            <input type="text" class="form-control" name="destination" value="<%= quote.destination %>" >
                          </div>
        
                        <div class="form-group">
                            <label for="name" >Name</label>
                            <input type="text" class="form-control" name="name" value="<%= quote.tag %>" >
                        </div>
                        
                        <div class="form-group">
                            <button type="submit" class="btn text-dark update">Save</button>
                        </div>
        
                </form>
               
            </div>
        </div>
        </div>
    </div>

    
    
</body>
